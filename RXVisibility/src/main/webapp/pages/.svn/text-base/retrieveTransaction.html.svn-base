<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
<body>
	<table width="100%">
		<tr>

			<td width="100%">
				<table class="table table-bordered">
					<tr>
						<td>
							<div style="background-color: #FFFFFF" ng-init="tab=searchPage">
								<div align="left" autofocus="true" class="alert alert-danger" ng-show="errorDescriptionSearchList.length > 0">
									
															<table style="width: 100%" align="center">
																<tr align="left"
																	ng-repeat="errorItem in errorDescriptionSearchList">
																	<td><span><b><font
																					color="#FF0000" size="2%">{{errorItem}}</font> </b> </span>
																	</td>
																</tr>
															</table>
								</div>
								<div ng-show="searchPage == 1">
									<table width="80%" align="center">
										<tr>
											<td>&nbsp;</td>
											<td>&nbsp;</td>
											<td>&nbsp;</td>
											<td>&nbsp;</td>
											<td>&nbsp;</td>
										</tr>
									</table>
									<table width="80%" align="center">
										<tr>
											<td>&nbsp;</td>
											<td>&nbsp;</td>
											<td>&nbsp;</td>
											<td>&nbsp;</td>
											<td>&nbsp;</td>
										</tr>
									</table>
									<div>
										<font color="#FF0000" size="4%">{{errorMessage}}</font>
									</div>
									<table width="90%">
										<tr>
											<td>

												<table width="80%" align="center">
													<tr>
														<td width="60%" class="table table-bordered">
															<table style="width: 90%" align="center">
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>
																<tr align="center">
																	<td colspan="2"><b>{{fieldValue.genrep_retrieve_instruction}}</b>
																	</td>
																</tr>
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>

															</table>
														</td>
													</tr>
												</table></td>
										</tr>
									</table>
									<table width="90%">
										<tr>
											<td>
												<table width="80%" align="center">
													<tr>
														<td width="60%" class="table table-bordered"
															align="center">


															<table style="width: 90%" align="center">
																<tr>
																	<td width="30%"><b>{{fieldValue.genrep_search_by}}</b>
																	</td>
																	<td width="2%">&nbsp;</td>
																	<td width="68%">&nbsp;</td>
																</tr>
																<tr>
																	<td class="form-control moz-placeholder" align="left"
																		width="30%"><b>{{fieldValue.genrep_national_drug_code}}</b>
																	</td>
																	<td width="2%">&nbsp;</td>
																	<td align="left" width="68%" colspan="4"><input
																		type="text" class="form-control moz-placeholder"
																		value="" ng-model="searchCriterias.ndcNbr">
																	</td>
																</tr>
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>
																<tr>
																	<td class="form-control moz-placeholder" align="left"
																		width="30%"><b>{{fieldValue.genrep_item_number}}</b>
																	</td>
																	<td width="2%">&nbsp;</td>
																	<td align="left" width="68%" colspan="4"><input
																		type="text" class="form-control moz-placeholder"
																		value="" maxlength="10"
																		ng-model="searchCriterias.itemNbr">
																	</td>
																</tr>
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>
																<tr>
																	<td class="form-control moz-placeholder" align="left"
																		width="30%"><b>{{fieldValue.genrep_purchase_order_number}}</b>
																	</td>
																	<td width="2%">&nbsp;</td>
																	<td align="left" width="68%" colspan="4"><input
																		type="text" class="form-control moz-placeholder"
																		value="" maxlength="10"
																		ng-model="searchCriterias.poNbr">
																	</td>
																</tr>
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>
																<tr>
																	<td class="form-control moz-placeholder" align="left"
																		width="30%"><b>{{fieldValue.genrep_vendor_number}}</b>
																	</td>
																	<td width="2%">&nbsp;</td>
																	<td align="left" width="68%" colspan="4"><input
																		type="text" class="form-control moz-placeholder"
																		value="" ng-model="searchCriterias.vendorNbr">
																	</td>
																</tr>
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>
																<tr>
																	<td class="form-control moz-placeholder" align="left"
																		width="30%"><b>{{fieldValue.genrep_trasnsaction_date}}</b><font
																		color="#FF0000" size="4%">*</font></td>
																	<td width="2%">&nbsp;</td>

																	<td align="left" width="68%">
																		<table>
																			<tr>
																				<td align="left"><b>From</b></td>
																				<td><input type="date"
																					class="form-control moz-placeholder" value=""
																					ng-model="searchCriterias.transactionFromDate">
																				</td>
																				<td align="left"><b>To</b></td>
																				<td><input type="date"
																					class="form-control moz-placeholder" value=""
																					ng-model="searchCriterias.transactionToDate">
																				</td>

																			</tr>
																		</table></td>


																</tr>
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>
																<tr>
																	<td class="form-control moz-placeholder" align="left"
																		width="30%"><b>{{fieldValue.genrep_lot_number}}</b>
																	</td>
																	<td width="2%">&nbsp;</td>
																	<td align="left" width="68%" colspan="4"><input
																		type="text" class="form-control moz-placeholder"
																		value="" maxlength="20"
																		ng-model="searchCriterias.lotNbr">
																	</td>
																</tr>
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>
																<tr>
																	<td class="form-control moz-placeholder" align="left"
																		width="30%"><b>{{fieldValue.genrep_location_number}}</b>
																	</td>
																	<td width="2%">&nbsp;</td>
																	<td " align="left" width="68%" colspan="4"><input
																		type="text" class="form-control moz-placeholder"
																		value="" maxlength="10"
																		ng-model="searchCriterias.locationNbr">
																	</td>
																</tr>
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>
															</table>

															<table style="width: 90%" align="center">
																<tr align="center">
																	<td width="30%">&nbsp;</td>
																	<td colspan="1"><input type="button"
																		ng-disabled="captureTransationForm.$invalid"
																		value="{{fieldValue.genrep_search}}"
																		ng-click="search()" class="btn btn-secondary"></input>
																		<!-- <div  ng-show="errorSwitch_PO == 1"  style="color:white;font-size: 80%">1</div></b>
												<div  ng-show="errorSwitch_NDC == 1"  style="color:white;font-size: 80%">1</div></b> -->
																	<td>
																	<td colspan="1"><input type="button"
																		ng-disabled="captureTransationForm.$invalid"
																		value="{{fieldValue.genrep_clear_screen}}"
																		ng-click="clearsearch()" class="btn btn-secondary"></input>
																		<!-- <div  ng-show="errorSwitch_PO == 1"  style="color:white;font-size: 80%">1</div></b>
												<div  ng-show="errorSwitch_NDC == 1"  style="color:white;font-size: 80%">1</div></b> -->
																	<td>
																	
																	<td colspan="1" width="30%">&nbsp;</td>
																</tr>
																<tr>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																	<td>&nbsp;</td>
																</tr>
															</table></td>
												</table></td>
										</tr>

									</table>
									<table>
										<tr>
											<td>&nbsp;</td>
											<td>&nbsp;</td>
											<td>&nbsp;</td>

										</tr>
									</table>

								</div>
								<div ng-show="searchPage == 2">
									<div ng-include="searchResult"></div>
								</div>
								<div ng-show="searchPage == 3">
									<div ng-include="shipmentResult"></div>
								</div>

							</div>
						</td>
					</tr>
				</table>
				<table>
					<tr>
						<td>&nbsp;</td>
						<td>&nbsp;</td>
						<td>&nbsp;</td>

					</tr>
				</table>
			</td>
		</tr>
	</table>
</body>
</html>