	<form name="loginForm" class="table table-bordered"
		style="width: 40%; background-color: white; border: 1px outset #007dc6; font-size: 100%; border-radius: 15px 15px 15px 15px;">

		<div
			style="background-color: #003896; color: white; height: 200%; border-radius: 15px 15px 0px 0px;"
			ng-switch=errorSwitch>
			<b>Login</b>
		</div>
		<div align="center" autofocus="true">
			<table width="70%">
				<tr align="left" ng-repeat="errorItem in errorDescriptionList">
					<td><span><b><font color="#FF0000" size="1%">{{errorItem}}</font>
						</b> </span></td>
				</tr>
			</table>
		</div>
		<table style="width: 60%" align="center">
			<tr>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
			</tr>
			<tr>
				<td style="color: black"><b>{{fieldValue.genrep_login_label_username}}
						<font color="#FF0000" size="4%">*</font> <input type="text"
						class="form-control moz-placeholder"
						ng-model="genRepPageController.username" class="text" required=""
						autofocus="autofocus" />
				</b>
					<div ng-show="genrepLoginError == 1"
						style="color: red; font-size: 80%">{{fieldValue.genrep_login_errMsg_username}}</div>
				</td>

			</tr>
			<tr ng-hide="genrepLoginError == 1">
				<td>&nbsp;</td>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
			</tr>

			<tr>
				<td style="color: black"><b>{{fieldValue.genrep_login_label_password}}
						<font color="#FF0000" size="4%">*</font> <input type="password"
						class="form-control moz-placeholder"
						ng-model="genRepPageController.password" class="text" required="" />
				</b>
					<div ng-show="genrepLoginError == 1"
						style="color: red; font-size: 80%">{{fieldValue.genrep_login_errMsg_password}}</div>
					</b></td>

			</tr>

			<tr ng-hide="genrepLoginError == 1">
				<td>&nbsp;</td>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
			</tr>

			<tr>
				<td style="color: black"><b>{{fieldValue.genrep_login_label_code}}
						<font color="#FF0000" size="4%">*</font> <!-- <select class="form-control moz-placeholder"	ng-init="genRepPageController.countryCode = options[0]" ng-model="genRepPageController.countryCode" ng-options="option.name for option in countryCodeOptions"></select> -->
						<select class="form-control moz-placeholder" required=""  ng-change="callingBusinessUnit()" id="selectedValue"
						ng-model="genRepPageController.countryCode">
							<option value="1" selected>Walmart US</option>
							<option value="2">Sam's Club</option>
							<option value="3">Walmart Puerto Rico</option>
					</select>
				</td>

			</tr>

			<tr ng-hide="genrepLoginError == 1">
				<td>&nbsp;</td>
				<td>&nbsp;</td>
				<td>&nbsp;</td>
			</tr>
