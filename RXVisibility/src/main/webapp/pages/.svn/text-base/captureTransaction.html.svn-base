															<td width="5%">{{lineItem.itemShipQty}}</td>
															<td width="5%">{{lineItem.qtyUOMCode}}</td>
															<td width="5%">{{lineItem.packSize}}</td>
														</tbody>

													</table>
												</div></td>

										</table>
