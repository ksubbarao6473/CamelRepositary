											<tr>
												<td>&nbsp;</td>
												<td>&nbsp;</td>
												<td>&nbsp;</td>

											</tr>
										</table>
										<table align="center" width="30%">
											<td width="30%" colspan="2">&nbsp;</td>

											<td width="4%">&nbsp;</td>
											<td colspan="1"><input type="button"
												ng-class="{'disabled':  record == null}"
												value={{fieldValue.genrep_save}} ng-click="saveOption()"
